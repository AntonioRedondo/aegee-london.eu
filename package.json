{
	"name": "aegee-london",
	"author": "Antonio Redondo",
	"homepage": "https://aegee-london.eu",
	"description": "AEGEE-London official website",
	"repository": "antonioredondo/AEGEE-London",
	"license": "CC-BY-NC-ND-4.0",
	"type": "module",
	"main": "",
	"scripts": {
		"lint": "concurrently \"eslint --ignore-pattern \"docs\"\" \"htmlhint \"**/*.html\" --ignore=\"docs/**\"\" \"stylelint \"**/*.scss\" --ip \"z_atoms.scss\"\"",
		"start": "http-server docs -s",
		"dev": "concurrently \"npm start\" \"gulp dev\"",
		"build": "gulp",
		"prepare": "husky install"
	},
	"dependencies": {
		"skrollr": "^0.6.26",
		"skrollr-menu": "^1.0.3"
	},
	"devDependencies": {
		"concurrently": "^9.2.1",
		"eslint": "^9.39.1",
		"globals": "^16.5.0",
		"gulp": "^5.0.1",
		"gulp-atomizer": "^3.0.7",
		"gulp-concat": "^2.6.1",
		"gulp-file-include": "^2.3.0",
		"gulp-html-minifier-terser": "^7.1.0",
		"gulp-inline": "^0.1.3",
		"gulp-newer": "^1.4.0",
		"gulp-postcss": "^10.0.0",
		"gulp-replace": "^1.1.4",
		"gulp-sourcemaps": "^3.0.0",
		"htmlhint": "^1.7.1",
		"http-server": "^14.1.1",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.6",
		"postcss-assets": "^6.0.0",
		"precss": "^4.0.0",
		"stylelint": "^16.25.0",
		"stylelint-config-standard-scss": "^16.0.0"
	},
	"lint-staged": {
		"*.js": "eslint --ignore-pattern \"docs\"",
		"*.htm": "htmlhint \"**/*.html\" --ignore=\"docs/**\"",
		"*.scss": "stylelint --ip \"z_atoms.scss\""
	}
}
